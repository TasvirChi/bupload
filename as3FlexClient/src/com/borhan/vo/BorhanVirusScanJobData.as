package com.borhan.vo
{
	import com.borhan.vo.BorhanJobData;

	[Bindable]
	public dynamic class BorhanVirusScanJobData extends BorhanJobData
	{
		/**
		 **/
		public var srcFilePath : String = null;

		/**
		 **/
		public var flavorAssetId : String = null;

		/**
		 * @see com.borhan.types.BorhanVirusScanJobResult
		 **/
		public var scanResult : int = int.MIN_VALUE;

		/**
		 * @see com.borhan.types.BorhanVirusFoundAction
		 **/
		public var virusFoundAction : int = int.MIN_VALUE;

		override public function getUpdateableParamKeys():Array
		{
			var arr : Array;
			arr = super.getUpdateableParamKeys();
			arr.push('srcFilePath');
			arr.push('flavorAssetId');
			arr.push('scanResult');
			arr.push('virusFoundAction');
			return arr;
		}

		override public function getInsertableParamKeys():Array
		{
			var arr : Array;
			arr = super.getInsertableParamKeys();
			return arr;
		}
	}
}
